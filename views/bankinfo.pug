extends layout

block content
  h1= title
  .main.container
    .row
      .col-md-6.col-md-offset-3
        form(method='POST' action='')
          div.form-group
            label(for='loginid') Login ID: 
            input.form-control(type='text' id='loginid' value=bag.Loginid name='loginid')
          div.form-group
            label(for='Profilename') Profile Name: 
            input.form-control(type='text' id='Profilename' value=bag.Profilename name='Profilename')
          div.form-group
            label(for='Accountype') Accoun Type: 
            input.form-control(type='text' id='Accountype' value=bag.Accountype='Accountype')
          div.form-group
            label(for='Bankname') Bank Name: 
            input.form-control(type='text' id='Bankname' value=bag.Bankname name='Bankname')
          div.form-group
            label(for='Routing') Routing:
            input.form-control(type='text' id='Routing' value=bag.Routing name='Routing')
          div.form-group
            label(for='Account') Account: 
            input.form-control(type='text' id='Account' value=bag.Account name='Account')
          div.form-group
            label(for='Validated') Validated: 
            input.form-control(type='text' id='Validated' value=bag.Validated name='Validated')
          div.form-group
            label(for='Sharedlogin') Shared with Login: 
            input.form-control(type='text' id='Sharedlogin' value=bag.Sharedlogin name='Sharedlogin')